<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_onego"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ritik.e8085.Onego"
    android:background="#2d2f30">

    <ListView
        android:layout_width="370dp"
        android:layout_height="350dp"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/listview_onego" />

    <Button
        android:text="Execute"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button_onego"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/listview_onego"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/btn_frame_trans"
        android:textColor="#f2f5f7"
        android:fontFamily="bold"
        android:textStyle="bold"/>

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_onego"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/button_onego"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        android:visibility="invisible"
        android:textColor="#AAdbdee1"/>
</android.support.constraint.ConstraintLayout>
